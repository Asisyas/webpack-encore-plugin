{
    "name": "boo/webpack-encore-plugin",
    "description": "This is a Micro plugin for webpack-encore support",
    "type": "library",
    "version": "0.1",
    "license": "MIT",
    "require": {
        "micro/plugin-twig": "^0"
    },
    "require-dev": {
